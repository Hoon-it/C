#include <iostream>

using namespace std;

int addTwoNumbers(int num_a, int num_b) //더하기 함수
{
  int sum = num_a + num_b;
  return sum;
}

int multiplyTwoNumbers(int num_a, int num_b)  //곱하기 함수
{
  int sum = num_a * num_b;
  return sum;
}

int main()
{
  cout << multiplyTwoNumbers(1,2) << endl;
  cout << addTwoNumbers(15,6) << endl;
  
  return 0;
}
